{
    "name": "Consumption",
    "description": "This view is intended for displaying the list of consumptions.",
    "access": {
        "groups": ["booking.default.user"]
    },
    "group_by": ["date",  {
        "field": "product_model_id",
        "operation": ["SUM", "object.qty"]
    }],
    "domain": ["type", "=", "book"],
    "limit": 100,
    "filters": false,
    "layout": {
        "items": [
            {
                "type": "field",
                "value": "date",
                "width": "10%",
                "visible": false
            },
            {
                "type": "field",
                "label": "Product",
                "value": "product_model_id",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "Age",
                "value": "age_range_id",
                "width": "15%"
            },
            {
                "type": "field",
                "value": "qty",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "schedule_from",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "schedule_to",
                "width": "10%"
            },
            {
                "type": "field",
                "value": "is_rental_unit",
                "width": "10%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "rental_unit_id",
                "width": "15%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "is_meal",
                "width": "10%",
                "sortable": true
            },
            {
                "type": "field",
                "value": "disclaimed",
                "width": "10%"
            }
        ]
    }
}