{
    "name": "PackLines",
    "description": "This view is intended for displaying the list of packlines.",
    "access": {
        "groups": ["pos.default.user", "sale.default.user"]
     },
    "layout": {
        "items": [
            {
                "type": "field",
                "label": "Child's Product",
                "value": "child_product_id",
                "width": "20%"
            },
            {
                "type": "field",
                "label": "Has Quantity",
                "value": "has_own_qty",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "Product Quantity",
                "value": "own_qty",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "Has Duration",
                "value": "has_own_duration",
                "width": "15%"
            },
            {
                "type": "field",
                "label": "Product Duration",
                "value": "own_duration",
                "width": "15%"
            }
        ]
    }
}